{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",

        "semi": [
            "warn",
            "always"
        ],

        "no-unused-vars" : "off",
        "no-inner-declarations" : "off"
    }
}
